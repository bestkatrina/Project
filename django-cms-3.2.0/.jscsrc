{
    "preset": "crockford",

    "maximumLineLength": {
        "value": 120,
        "allowRegex": true,
        "allowUrlComments": true
    },
    "requireDotNotation": {
        "allExcept": ["keywords"]
    },
    "disallowQuotedKeysInObjects": null,
    "requireMultipleVarDecl": null,
    "disallowDanglingUnderscores": null,
    "requireVarDeclFirst": false
}
